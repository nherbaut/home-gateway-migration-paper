	pages = {44--51},
	file = {IEEE Xplore Abstract Record:/home/nherbaut/Documents/biblio/zotero/storage/VFHV7FBH/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/home/nherbaut/Documents/biblio/zotero/storage/J66CNWA9/Rimal et al. - 2009 - A Taxonomy and Survey of Cloud Computing Systems.pdf:application/pdf}
